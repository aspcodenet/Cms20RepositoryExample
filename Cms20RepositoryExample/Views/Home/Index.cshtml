@model List<Student>

@{
    ViewData["Title"] = "Home Page";
}

<h1>Alla studenter</h1>


<table class="table table-dark">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Namn</th>
        <th scope="col">Ålder</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
        <tr>
            <th scope="row">@student.Id</th>
            <td>@student.Name</td>
            <td>@student.Age</td>
        </tr>
        }
    </tbody>
</table>